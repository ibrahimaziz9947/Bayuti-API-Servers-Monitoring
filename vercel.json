{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/main.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/run-monitoring?secret=SomeVeryStrongSecretValue123",
      "schedule": "*/5 * * * *"
    }
  ]
}
